{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "src/**/__tests__/**",
    "src/**/__mocks__/**",
    "src/**/index.ts"
  ],
  "reporter": ["text", "text-summary", "html", "lcov"],
  "report-dir": "coverage-e2e",
  "temp-dir": "coverage-e2e/.nyc_output",
  "sourceMap": true,
  "instrument": false
}
